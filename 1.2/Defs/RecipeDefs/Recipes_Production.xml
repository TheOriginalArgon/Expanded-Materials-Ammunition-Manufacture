<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- Industrial Ammo -->
<RecipeDef>
    <defName>VMEu_MakeIndustrialAmmo</defName>
    <label>make industrial turret ammunition</label>
    <description>Manufacture industrial class ammunition for military class turrets.</description>
    <jobString>Manufacturing ammunition.</jobString>
    <effectWorking>Smith</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workAmount>500</workAmount>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
	<recipeUsers>
		<li>TableMachining</li>
	</recipeUsers>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>VMEu_Bronze</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>VMEu_Lead</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>VMEu_Bronze</li>
        <li>Steel</li>
        <li>VMEu_Lead</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <VMEu_IndustrialAmmo>25</VMEu_IndustrialAmmo>
    </products>
	<researchPrerequisite>Machining</researchPrerequisite>
</RecipeDef>

